<odoo>
	<record id="people3_view_form" model="ir.ui.view">
		<field name="name">people3.form</field>
		<field name="model">people3</field>
		<field name="arch" type="xml">
			<form string="People3">
				<header>
					<button name="action_check" string='Check' id="action_check_btn" type="object" class="oe_highlight" />
				</header>
				<sheet>
					<field name="name" />
					<group>
						<group>
							<field name="date" />
						</group>
						<group>
							<field name="datetime" />
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record id="people3_action" model="ir.actions.act_window">
		<field name="name">People3</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">people3</field>
	</record>

	<menuitem id="menu_field" name="Date_Field" />
	<menuitem id="menu_people3" name="Date Field" parent="menu_field" action='people3_action' />
</odoo>
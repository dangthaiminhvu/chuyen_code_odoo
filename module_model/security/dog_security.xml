<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <!-- Category -->
        <record id="module_category_dog" model="ir.module.category">
            <field name="name">Phan cap</field>
            <field name="sequence">1</field>
        </record>

        <!-- Group: Nguoi mua cho -->
        <record id="group_nguoi_mua_cho" model="res.groups">
            <field name="name">Nguoi_mua_cho</field>
            <field name="category_id" ref="module_category_dog"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <!-- Group: Nguoi quan ly cho -->
        <record id="group_nguoi_quan_ly_cho" model="res.groups">
            <field name="name">Nguoi_quan_ly_cho</field>
            <field name="category_id" ref="module_category_dog"/>
            <field name="implied_ids" eval="[(4, ref('group_nguoi_mua_cho'))]"/>
        </record>

    </data>
</odoo>

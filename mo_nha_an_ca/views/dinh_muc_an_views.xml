<odoo>
    <record id="view_dinh_muc_an_tree" model="ir.ui.view">
        <field name="name">dinh_muc_an.tree</field>
        <field name="model">mo_nha_an_ca.dinh_muc_an</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="nhom"/>
                <field name="vi_tri"/>
                <field name="so_suat"/>
            </tree>
        </field>
    </record>

    <record id="view_dinh_muc_an_form" model="ir.ui.view">
        <field name="name">dinh_muc_an.form</field>
        <field name="model">mo_nha_an_ca.dinh_muc_an</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="nhom"/>
                        <field name="vi_tri"/>
                        <field name="so_suat"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_dinh_muc_an" model="ir.actions.act_window">
        <field name="name">Định mức ăn</field>
        <field name="res_model">mo_nha_an_ca.dinh_muc_an</field>
        <field name="view_mode">tree,form</field>
        <field name="type">ir.actions.act_window</field>
    </record>

    <menuitem id="menu_dinh_muc_an" name="Định mức ăn" parent="menu_nha_an_ca_root" action="action_dinh_muc_an"/>
</odoo>
<odoo>
    <record id="view_cham_an_tree" model="ir.ui.view">
        <field name="name">cham_an.tree</field>
        <field name="model">mo_nha_an_ca.cham_an</field>
        <field name="arch" type="xml">
            <tree>
                <field name="phieu_quet_id"/>
                <field name="bao_an_id"/>
                <field name="so_suat_thuc_te"/>
                <field name="ly_do"/>
            </tree>
        </field>
    </record>

    <record id="view_cham_an_form" model="ir.ui.view">
        <field name="name">cham_an.form</field>
        <field name="model">mo_nha_an_ca.cham_an</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="phieu_quet_id"/>
                        <field name="bao_an_id"/>
                        <field name="so_suat_thuc_te"/>
                        <field name="ly_do"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_cham_an" model="ir.actions.act_window">
        <field name="name">Chấm ăn</field>
        <field name="res_model">mo_nha_an_ca.cham_an</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_cham_an" name="Chấm ăn" parent="menu_nha_an_ca_root" action="action_cham_an"/>

</odoo>
<odoo>
    <record id="view_bao_an_tree" model="ir.ui.view">
        <field name="name">bao_an.tree</field>
        <field name="model">mo_nha_an_ca.bao_an</field>
        <field name="arch" type="xml">
            <tree>
                <field name="ngay"/>
                <field name="ca"/>
                <field name="vi_tri"/>
                <field name="so_suat_du_kien"/>
                <field name="doi_tac_ngoai"/>
            </tree>
        </field>
    </record>

    <record id="view_bao_an_form" model="ir.ui.view">
        <field name="name">bao_an.form</field>
        <field name="model">mo_nha_an_ca.bao_an</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="ngay"/>
                        <field name="ca"/>
                        <field name="vi_tri"/>
                        <field name="so_suat_du_kien"/>
                        <field name="doi_tac_ngoai"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_bao_an" model="ir.actions.act_window">
        <field name="name">Báo Ăn</field>
        <field name="res_model">mo_nha_an_ca.bao_an</field>
        <field name="view_mode">tree,form</field>
        <field name="type">ir.actions.act_window</field>
    </record>

    <menuitem id="menu_bao_an" name="Báo Ăn" parent="menu_nha_an_ca_root" action="action_bao_an"/>
</odoo>
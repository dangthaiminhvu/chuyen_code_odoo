<odoo>
    <record id="view_dang_ky_suat_an_tree" model="ir.ui.view">
        <field name="name">dang_ky_suat_an.tree</field>
        <field name="model">mo_nha_an_ca.dang_ky_suat_an</field>
        <field name="arch" type="xml">
            <tree>
                <field name="employee_id"/>
                <field name="dinh_muc_id"/>
                <field name="ngay"/>
                <field name="so_suat"/>
                <field name="loai_suat"/>
            </tree>
        </field>
    </record>

    <record id="view_dang_ky_suat_an_form" model="ir.ui.view">
        <field name="name">dang_ky_suat_an.form</field>
        <field name="model">mo_nha_an_ca.dang_ky_suat_an</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="employee_id"/>
                        <field name="dinh_muc_id"/>
                        <field name="ngay"/>
                        <field name="so_suat"/>
                        <field name="loai_suat"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
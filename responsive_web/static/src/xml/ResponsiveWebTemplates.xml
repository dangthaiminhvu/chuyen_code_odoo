<?xml version="1.0" encoding="utf-8"?>
<t t-name="responsive_web.WebResponsiveTmp">
    <div class="custom-home-background">
        <div class="oh_dashboards" t-ref="root">
            <div class="dashboard_main o_web_responsive">
                <t t-if="state.showModal">
                    <div class="nav-container" style="position: relative; z-index: 10000;">
                        <t t-call="responsive_web.SearchResult"/>
                    </div>
                </t>
                <t t-else="">
                    <div class="nav-container" style="position: relative; z-index: 10000;">
                        <div class="app-menu">
                            <t t-as="app" t-foreach="state.menus" t-key="app_index">
                                <a class="dropdown-item o_app mt0" role="menuitem" t-att-data-action-id="app.actionID" t-att-data-menu-id="app.menuID" t-att-data-menu-xmlid="app.xmlID" t-attf-href="#menu_id={{app.id}}">
                                    <span class="img-wrap">
                                        <img style="width:48px;height:48px;object-fit:contain;" t-att-title="app.name" t-attf-src="data:image/png;base64, {{app.webIconData}}" t-if="app.webIconData"/>
                                    </span>
                                    <b class="a_app_menu_title"><t t-esc="app.name"/></b>
                                </a>
                            </t>
                        </div>
                    </div>
                </t>
            </div>
        </div>
    </div>
</t>